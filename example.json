{
  "AWSVersion": "2022-04-02",
  "Name": "Sinusoidal Simulator to DEBUG Configuration",
  "Description": "Configuration for displaying sine wave simulated data in debug output",
  "LogLevel": "Info",
  "Schedules": [
    {
      "Name": "SinusDataSchedule",
      "Interval": 5000,
      "Active": true,
      "TimestampLevel": "Both",
      "Sources": {
        "SIMULATOR-SOURCE": [
          "sinus"
        ]
      },
      "Targets": [
        "DEBUGTarget"
      ]
    }
  ],
  "Sources": {
    "SIMULATOR-SOURCE": {
      "Name": "SinusoidalSimulator",
      "ProtocolAdapter": "SIMULATOR",
      "Description": "Sine wave simulator source",
      "Channels": {
        "sinus": {
          "Simulation": {
            "SimulationType": "Sinus",
            "DataType": "Float",
            "Min": -10.0,
            "Max": 10.0,
            "Frequency": 0.1
          }
        }
      }
    }
  },
  "Targets": {
    "DEBUGTarget": {
      "Active": true,
      "TargetType": "DEBUG-TARGET",
      "Pretty": true
    }
  },
  "TargetTypes": {
    "DEBUG-TARGET": {
      "JarFiles": [
        "${MODULES_DIR}/debug-target/lib"
      ],
      "FactoryClassName": "com.amazonaws.sfc.debugtarget.DebugTargetWriter"
    }
  },
  "AdapterTypes": {
    "SIMULATOR": {
      "JarFiles": [
        "${MODULES_DIR}/simulator/lib"
      ],
      "FactoryClassName": "com.amazonaws.sfc.simulator.SimulatorAdapter"
    }
  },
  "ProtocolAdapters": {
    "SIMULATOR": {
      "AdapterType": "SIMULATOR"
    }
  }
}